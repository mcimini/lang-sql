Label L ::= (a) | (b)
Process P ::= (null) | (a P) | (b P) | (disrupt P P) 

(a P) --(a)--> P.
(b P) --(b)--> P.

(disrupt P1 P2) --(a)--> (disrupt P1' P2) <== P1 --(a)--> P1'.
(disrupt P1 P2) --(a)--> P2' <== P2 --(a)--> P2'.
(disrupt P1 P2) --(b)--> (disrupt P1' P2) <== P1 --(b)--> P1'.
(disrupt P1 P2) --(b)--> P2' <== P2 --(b)--> P2'.

